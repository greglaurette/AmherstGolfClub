@model IEnumerable<AmherstGolfClub.Models.EventType>

@{
    ViewBag.Title = "Event Type Admin";
}

<h2>Event Type Admin</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Type)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Type)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.EventTypeID }) |
            @Html.ActionLink("Details", "Details", new { id=item.EventTypeID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.EventTypeID })
        </td>
    </tr>
}

</table>
